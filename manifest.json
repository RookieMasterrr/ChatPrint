{
    "manifest_version": 3,
    "name": "ChatPrint",
    "description": "turn your chatgpt answer to pdf",
    "version": "1.1",
    "icons": {
        "16": "icons/icon-16.png",
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://chatgpt.com/*"
            ],
                "js": ["./scripts/html2pdf.bundle.min.js", "./scripts/content.js"],
            "type": "module"
        }
    ]
}